                $('#opCarrera').append('<option class="not_chosen" value="ITC">Ingeniería en Tecnologías Computacionales</option>');
                $('#opCarrera').append('<option class="not_chosen" value="IIS">Ingeniería Industrial y de Sistemas</option>');
                $('#opCarrera').show();
                break;

            case 'UN':
                $('#opCarrera').append('<option class="not_chosen" value="IIS">Ingeniería Industrial y de Sistemas</option>')
                $('#opCarrera').append('<option class="not_chosen" value="ITI">Ingeniería en Tecnologías de Información</option>');
                $('#opCarrera').append('<option class="not_chosen" value="IATI">Ingeniería en Administración de Tecnologías de Información</option>');
                $('#opCarrera').show();
                break;

            case 'UANL':
                $('#opCarrera').hide()
                $('#txtCarrera').val('');
                $('#txtCarrera').show();
                break;

            case 'OTRO':
                $('#opCarrera').hide()
                $('#txtCarrera').val('');
                $('#txtCarrera').show();
                break;
        }
    }


