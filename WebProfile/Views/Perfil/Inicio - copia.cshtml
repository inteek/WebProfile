    function splitDateOrder(fecha) {

        //debugger;

        var srtFecha = fecha.split('/');

        //alert('srtFecha.length: ' + srtFecha.length);

        if (srtFecha.length == 3) {
            var dia = parseInt(srtFecha[0]);
            var mes = parseInt(srtFecha[1]);
            var anio = parseInt(srtFecha[2]);


            if (dia > 31) {
                alert("La fecha de nacimiento no es valida, \nel día es incorrecto (dd/mm/yyyy)");
                return "";
            }

            if (mes > 12) {
                alert("La fecha de nacimiento no es valida, \nel mes es incorrecto (dd/mm/yyyy)");
                return "";
            }

            if (anio < 1000) {
                alert("La fecha de nacimiento no es valida, \nel año es incorrecto (dd/mm/yyyy)");
                return "";
            }

            return mes + "/" + dia + "/" + anio

        } else {
            alert("La fecha de nacimiento no es valida (dd/mm/yyyy)");
            return "";
        }

        //alert(srtFecha[1] + "/" + srtFecha[0] + "/" + srtFecha[2]);

    }


