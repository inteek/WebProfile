    function validateDocumentSizeType(input) {
        //debugger
        if (input.files && input.files[0]) {
            //File { name: "Report0.pdf", size: 149851, type: "application/pdf" }
            if (input.files[0].type != "application/pdf" && input.files[0].type != "image/jpeg" && input.files[0].type != "image/jpg" && input.files[0].type != "image/png" && input.files[0].type != "image/x-ms-bmp"
                && input.files[0].type != "application/zip" && input.files[0].type != "application/x-zip-compressed" && input.files[0].type != "multipart/x-zip"
                && input.files[0].type != "application/rar" && input.files[0].type != "application/x-rar-compressed" && input.files[0].type != "compressed/x-rar") {
                alert("Solo se permiten archivos de tipo:\n pdf, jpeg, png, jpg, bmp, zip y rar");
                return false;
            }
            if (input.files[0].size > 4194304) {
                alert("El tamaño maximo permitido es de 4 MB")
                return false;
            }
        }
        return true;
    }

    function validateImageSizeType(input) {
        //debugger
        if (input.files && input.files[0]) {
            //File { name: "Report0.pdf", size: 149851, type: "application/pdf" }
            if (input.files[0].type != "image/jpg" && input.files[0].type != "image/jpeg" && input.files[0].type != "image/png" && input.files[0].type != "image/x-ms-bmp") {
                alert("Solo se permiten archivos de tipo:\n jpeg, png, jpg y bmp");
                return false;
            }
            if (input.files[0].size > 2097152) {
                alert("El tamaño maximo permitido es de 2 MB")
                return false;
            }
        }
        return true;
    }

